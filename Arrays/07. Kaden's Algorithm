class Solution {
  public:
    int maxSubarraySum(vector<int> &arr) {
        int maxsum=INT_MIN;
        int curr=0;
        for(int i=0;i<arr.size();i++)
        {
            curr+=arr[i];
            maxsum=max(maxsum,curr);
            if(curr<0)
            {
                curr=0;
            }
        }
        return maxsum;
        
    }
};
