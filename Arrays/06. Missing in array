class Solution {
  public:
    int missingNum(vector<int>& arr) {
        // code here
        long long n=arr.size();
        long long sum_1=((n+1)*(n+2))/2;
        long long sum_2=0;
        for(int i=0;i<n;i++)
        {
            sum_2+=arr[i];
        }
        return (int)(sum_1-sum_2);
    }
};
