class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        // code here
        int i=0;
        int j=1;
        while(j<arr.size())
        {
            if(arr[i]==0 && arr[j]!=0)
            {
                swap(arr[j],arr[i]);
                i++;
                j++;
            }
            else if(arr[i]==0 && arr[j]==0)
            {
                j++;
            }
            else{
                i++;
                j++;
            }
        }
    }
};
